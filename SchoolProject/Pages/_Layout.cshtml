@using Microsoft.AspNetCore.Components.Web
@namespace SchoolProject.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="~/" />
    <link rel="stylesheet" href="css/btrap/bootstrap.min.css" />
    <link rel="stylesheet" href="css/mainBundle.css" />
    <link rel="stylesheet" href="SchoolProject.styles.css" />
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>
<body>
@RenderBody()

<div id="blazor-error-ui">
    <enviroment include="Staging, Production">
        An error has occurred. This app may no longer work until reloaded.
    </enviroment>
    <enviroment include="Development">
        An unhandled exception has occurred. See browser dev tools for details.
    </enviroment>
    <a href="" class="reload">Reload</a>
    <a href="" class="dismiss">&#88</a>
</div>

<script src="_framework/blazor.server.js"></script>
<script src="js/btrap/bootstrap.bundle.min.js"></script>
</body>
</html>